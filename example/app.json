{
  "expo": {
    "name": "Superwall Expo Example",
    "slug": "superwall-expo-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "superwall-expo-example",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.superwall.Advanced"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.superwall.superapp"
    },
    "plugins": ["expo-router", ["../app.plugin.js", { "apiKey": "custom_secret_api" }]],
    "experiments": {
      "typedRoutes": true
    }
  }
}
