{
  "name": "superwall-expo",
  "version": "0.0.1",
  "description": "Offical Expo Integration for Superwall",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/src/index.js",
      "types": "./build/src/index.d.ts"
    },
    "./compat": {
      "import": "./build/src/compat/index.js",
      "types": "./build/src/compat/index.d.ts"
    },
    "./package.json": "./build/package.json"
  },
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "format": "biome check --write ."
  },
  "keywords": [
    "react-native",
    "expo",
    "superwall-expo",
    "SuperwallExpo"
  ],
  "repository": "https://github.com/superwall/superwall-expo",
  "bugs": {
    "url": "https://github.com/superwall/superwall-expo/issues"
  },
  "author": "Makisuo <david@superwall.com> (https://github.com/makisuo)",
  "license": "MIT",
  "homepage": "https://github.com/superwall/superwall-expo#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/react": "~19.1.5",
    "expo": "53.0.9",
    "expo-module-scripts": "^4.1.7",
    "react-native": "0.79.2"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "files": [
    "build",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "ios",
    "android",
    "expo-module.config.json",
    "plugin",
    "app.plugin.js"
  ],
  "packageManager": "yarn@4.7.0"
}
